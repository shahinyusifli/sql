-- https://www.w3resource.com/mysql-exercises/insert-into-statement/index.php is source of exercies


-- EX 1
CREATE TABLE IF NOT EXISTS countries ( 
COUNTRY_ID varchar(2) NOT NULL UNIQUE PRIMARY KEY,
COUNTRY_NAME varchar(40) NOT NULL,
REGION_ID decimal(10,0) NOT NULL
);

INSERT INTO countries VALUES('US','INDIANA',5002);

SELECT * FROM countries;


-- EX 2
CREATE TABLE IF NOT EXISTS Countries(
COUNTRY_ID INT DEFAULT NULL, 
COUNTRY_NAME VARCHAR(40) DEFAULT NULL,
REGION_ID DECIMAL(10,0)
);

INSERT INTO Countries(COUNTRY_ID, COUNTRY_NAME) VALUES(1, 'US');

-- EX 3
CREATE TABLE IF NOT EXISTS Countries(
COUNTRY_ID INT DEFAULT NULL, 
COUNTRY_NAME VARCHAR(40) DEFAULT NULL,
REGION_ID DECIMAL(10,0)
);

CREATE TABLE IF NOT EXISTS NEW_COUNTRY AS SELECT * FROM Countries;


-- EX 4
INSERT INTO Countries VALUES(10, 'US', NULL);


-- EX 5
INSERT INTO Countries VALUES(3, 'US', NULL), (8, 'UK', 4000);


-- EX 6
CREATE TABLE IF NOT EXISTS Countries(
COUNTRY_ID INT DEFAULT NULL, 
COUNTRY_NAME VARCHAR(40) DEFAULT NULL,
REGION_ID DECIMAL(10,0)
);

CREATE TABLE IF NOT EXISTS New_Countries LIKE Countries;

INSERT INTO Countries VALUES(3, 'US', NULL), (8, 'UK', 4000);

INSERT INTO New_Countries SELECT * FROM Countries;


-- EX 7
CREATE TABLE IF NOT EXISTS jobs ( 
JOB_ID integer NOT NULL UNIQUE , 
JOB_TITLE varchar(35) NOT NULL, 
MIN_SALARY decimal(6,0)
);

INSERT INTO jobs VALUES(1, 'managment', 1200);


-- EX 8
INSERT INTO jobs(JOB_ID, JOB_TITLE, MIN_SALARY) VALUES(4, 'searching', 5200);


-- EX 9
CREATE TABLE IF NOT EXISTS Countries(
COUNTRY_ID INT DEFAULT NULL, 
COUNTRY_NAME VARCHAR(40) DEFAULT NULL,
REGION_ID DECIMAL(10,0)
);

INSERT INTO Countries(REGION_ID, COUNTRY_ID) VALUES(20, 70);


-- EX 10
CREATE TABLE IF NOT EXISTS Countries(
COUNTRY_ID integer NOT NULL AUTO_INCREMENT PRIMARY KEY, 
COUNTRY_NAME VARCHAR(40) DEFAULT NULL,
REGION_ID DECIMAL(10,0)
);

INSERT INTO Countries(COUNTRY_NAME, REGION_ID) VALUES('France', 1504);


-- EX 11
CREATE TABLE IF NOT EXISTS Countries(
COUNTRY_ID integer NOT NULL AUTO_INCREMENT PRIMARY KEY, 
COUNTRY_NAME VARCHAR(40) DEFAULT 'N/A',
REGION_ID DECIMAL(10,0)
);

INSERT INTO Countries(COUNTRY_NAME, COUNTRY_ID) VALUES('BULDING', 5000);


-- EX 12
















































